
.outer{
  margin: 20px;
  height: 430px; 
  display: flex;
  flex-direction: column;
  background-color: #e00;
  border-radius: 10px;
  align-items: center;
  justify-content: center; 
}
.middle{
  display: flex;
  width: 200px;
  height: 200px;
  border-radius: 10px;
  flex-direction: column;
  background-color: #0e0;
  align-items: center;
  justify-content: center;
}

.inner{
  display: flex;
  width: 150px;
  height: 100px;
  border-radius: 10px;
  justify-content: center;
  align-items: center;
  background-color: #00e;
}